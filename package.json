{
  "name": "@federicocarboni/scarpet-docs",
  "version": "0.1.0-dev",
  "main": "index.js",
  "type": "module",
  "types": "index.d.ts",
  "exports": {
    ".": "./index.js"
  },
  "files": ["index.d.ts"],
  "scripts": {
    "lint:md": "markdownlint-cli2 \"_*/*.md\" && prettier -c \"**/*.md\"",
