    "prepack": "hugo --minify && node scripts/genIndex.cjs"
  },
  "keywords": [],
  "author": "Federico Carboni",
  "devDependencies": {
    "markdownlint-cli2": "^0.12.1",
    "prettier": "^3.2.4",
    "prettier-plugin-go-template": "^0.0.15"
  }
}
